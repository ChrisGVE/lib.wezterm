#!/bin/sh

LUAROCKS_SYSCONFDIR='/usr/local/etc/luarocks' exec '/usr/local/opt/lua/bin/lua5.4' -e 'package.path="/Users/chris/dev/projects/plugins/wezterm/lib.wezterm/.lua/share/lua/5.4/?.lua;/Users/chris/dev/projects/plugins/wezterm/lib.wezterm/.lua/share/lua/5.4/?/init.lua;"..package.path;package.cpath="/Users/chris/dev/projects/plugins/wezterm/lib.wezterm/.lua/lib/lua/5.4/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("luacheck","1.2.0-1")' '/Users/chris/dev/projects/plugins/wezterm/lib.wezterm/.lua/lib/luarocks/rocks-5.4/luacheck/1.2.0-1/bin/luacheck' "$@"
